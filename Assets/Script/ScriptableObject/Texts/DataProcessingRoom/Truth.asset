  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u3044\u3084\u3001\u5F85\u3066\uFF01\u7D50\u5C40\u4FFA\u305F\u3061\u306F\u306A\u306B\u306B\u5DFB\u304D\u8FBC\u307E\u308C\u305F\u3093\u3060\uFF01\n\u3053\u306E\u30B3\u30ED\u30B7\u30A2\u30A4\u306F\u306A\u3093\u306A\u3093\u3060\uFF01"
    colorType: 0
