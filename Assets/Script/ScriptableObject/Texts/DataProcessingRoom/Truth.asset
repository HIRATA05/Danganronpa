    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u2026\u306A\u3093\u3060\u3068\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u3060\u3088\u3001\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u3002\u30AA\u30DE\u30A8\u30E9\u77E5\u3089\u306A\u3044\u306E\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30EC\u30A4\u30BC\u30A4\u30EA\u30F3"
    paragraphs: "\u306A\u3093\u3067\u3059\u304B\u2026\uFF1F\u305D\u308C\u3063\u3066\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u2026\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u3002\u4FFA\u306F\u305D\u306E\u8A00\u8449\u3092\u898B\u305F\u3053\u3068\u304C\u3042\u308B\u306F\u305A\u3060\u2026"
    colorType: 1
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u3053\u306E\u30B3\u30ED\u30B7\u30A2\u30A4\u306E\u540D\u524D\u3060\u306A\u2026"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u3068\u3044\u3046\u540D\u524D\u306E\u30B3\u30ED\u30B7\u30A2\u30A4\u3092\n\u4F55\u5EA6\u3082\u7E70\u308A\u8FD4\u3057\u3066\u304D\u305F\u3093\u3060\uFF01"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u3067\u3082\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u30B7\u30EA\u30FC\u30BA\u306F\u3082\u3046\u7D42\u308F\u3063\u305F\u3093\u3060\u3088\u306D\u2026"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B3\u30C8\u30AE \u30DE\u30CA"
    paragraphs: "\u7D42\u308F\u3063\u305F\u3063\u3066\u3069\u3046\u3044\u3046\u3053\u3068\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u306F\u524D\u56DE\u3067\u5B8C\u5168\u306B\u7D42\u4E86\u3057\u305F\u3093\u3060\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u3060\u304B\u3089\u5F7C\u5973\u306F\u307F\u3093\u306A\u306E\u5E0C\u671B\u3067\u3042\u308B\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u3092\u518D\u751F\u3055\u305B\u3088\u3046\u3068\u3057\u305F\u3093\u3060\u306D\u2026"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u307E\u3001\u4E0D\u904B\u306A\u3053\u3068\u306B\u30B3\u30ED\u30C3\u3068\u6B7B\u3093\u3058\u3083\u3063\u3066\u5922\u306F\u65AD\u305F\u308C\u305F\u308F\u3051\u3067\u3059\u304C\uFF01"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u307F\u3093\u306A\u306E\u5E0C\u671B\u2026\uFF1F\u304A\u524D\u306F\u306A\u306B\u3092\u3044\u3063\u3066\u3044\u308B\uFF1F\n\u3053\u3093\u306A\u3053\u3068\u304C\u8AB0\u304B\u306E\u5E0C\u671B\u306B\u306A\u308B\u308F\u3051\u304C\u306A\u3044\u3060\u308D\u3046\uFF01"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u3042\u30FC\u3082\u2015\u3046\u308B\u3055\u3044\u306A\u3042\u2026\n"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u7528\u610F\u3057\u305F\u4F0F\u7DDA\u3082\u5168\u90E8\u53F0\u7121\u3057\u306B\u306A\u3063\u3066\u3001\n\u3053\u3093\u306A\u7D50\u672B\u306A\u3093\u3066\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u3089\u3057\u304F\u306A\u3044\u3088\uFF01"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u3053\u308C\u3067\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u306F\u672C\u5F53\u306B\u7D42\u308F\u3063\u305F\u3093\u3060\uFF01\u7D42\u4E86\uFF01\u9589\u5EF7\uFF01"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u672C\u5F53\u306B\u3053\u308C\u3067\u7D42\u308F\u308B\u306E\u304B\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u3046\u3077\u3077\u2026\u5B89\u5FC3\u3057\u3066\u3088\u3002\n\u3082\u3046\u30B3\u30ED\u30B7\u30A2\u30A4\u304C\u7E70\u308A\u8FD4\u3055\u308C\u308B\u3053\u3068\u306F\u7121\u3044\u3093\u3060\u304B\u3089\u3055\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30E2\u30CE\u30AF\u30DE"
    paragraphs: "\u3053\u306E\u4E16\u754C\u3067\u30C0\u30F3\u30AC\u30F3\u30ED\u30F3\u30D1\u306F\u3082\u3046\u751F\u307E\u308C\u306A\u3044\u3093\u3060\u2026\u6B8B\u5FF5\u3060\u3051\u3069\u306D\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u305D\u3046\u4E00\u65B9\u7684\u306B\u6253\u3061\u5207\u3063\u3066\u30E2\u30CE\u30AF\u30DE\u306F\u59FF\u3092\u6D88\u3057\u305F\u3002"
    colorType: 1
    windowType: 3
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u305D\u3057\u3066\u30B3\u30ED\u30B7\u30A2\u30A4\u5B66\u5712\u751F\u6D3B\u306F\n\u591A\u304F\u306E\u8B0E\u3092\u6B8B\u3057\u305F\u307E\u307E\u3042\u3063\u3055\u308A\u3068\u7D42\u308F\u3063\u305F\u3002"
    colorType: 1
    windowType: 3
