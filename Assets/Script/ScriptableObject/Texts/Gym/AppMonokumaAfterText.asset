  textInfomations:
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u305D\u3093\u306A\u8A00\u8449\u3092\u5410\u3044\u3066\u3001\u4E0D\u6C17\u5473\u306A\u30AF\u30DE\u2026\u30E2\u30CE\u30AF\u30DE\u306F\u3069\u3053\u304B\u3078\u6D88\u3048\u305F\u3002"
    colorType: 1
    windowType: 0
  - speakerName: "\u30EC\u30A4\u30BC\u30A4 \u30EA\u30F3"
    paragraphs: "\u3053\u3001\u30B3\u30ED\u30B7\u30A2\u30A4\uFF1F\u3069\u3046\u3044\u3046\u3053\u3068\u3067\u3059\u304B\u2026\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30DF\u30CA\u30BB \u30C4\u30DF\u30AB"
    paragraphs: "\u3044\u304D\u306A\u308A\u62C9\u81F4\u3089\u308C\u3066\u2026\u5B66\u6821\u307F\u3066\u3048\u306A\u5834\u6240\u3067\u3044\u304D\u306A\u308A\u6BBA\u3057\u5408\u3044\u3060\u3068\u2026"
    colorType: 0
    windowType: 0
  - speakerName: "\u30DF\u30CA\u30BB \u30C4\u30DF\u30AB"
    paragraphs: "\u30AF\u30BD\u3063\uFF01\u3075\u3056\u3051\u305F\u3053\u3068\u8A00\u3044\u3084\u304C\u3063\u3066\u2026"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u307F\u3093\u306A\u2026\u307E\u305A\u306F\u843D\u3061\u7740\u3051\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30CB\u30AB\u30A4\u30C9\u30A6 \u30CF\u30EB\u30CE"
    paragraphs: "\u305D\u3046\u3055\u3001\u3053\u3093\u306A\u6642\u3053\u305D\u51B7\u9759\u306B\u306A\u306B\u3092\u3059\u308B\u3079\u304D\u304B\u8003\u3048\u306A\u3044\u3068\u2026\n\u7591\u5FC3\u6697\u9B3C\u306B\u306A\u3063\u3066\u306F\u30E2\u30CE\u30AF\u30DE\u306E\u601D\u3046\u3064\u307C\u3060\u304B\u3089\u306D\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30EC\u30A4\u30BC\u30A4 \u30EA\u30F3"
    paragraphs: "\u305D\u3046\u306F\u8A00\u3063\u3066\u3082\u2026\u306A\u306B\u3092\u3059\u308C\u3070\u3044\u3044\u3093\u3067\u3059\u304B\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "1\u3064\u306F\u4EF2\u9593\u3092\u6BBA\u3055\u306A\u3044\u3053\u3068\u3060\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u4E8C\u968E\u5802\u306E\u8A00\u3046\u901A\u308A\u3001\u6BBA\u3057\u5408\u3044\u3092\u59CB\u3081\u305F\u3089\u30E2\u30CE\u30AF\u30DE\u306E\u601D\u3046\u3064\u307C\u3060\u304B\u3089\u306A\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u3053\u308C\u3060\u3051\u306F\u907F\u3051\u306A\u3044\u3068\u3044\u3051\u306A\u3044\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u305D\u3057\u3066\u3082\u3046\u4E00\u3064\u2026\u3053\u306E\u5B66\u6821\u304B\u3089\u8131\u51FA\u3059\u308B\u65B9\u6CD5\u3092\u63A2\u3059\u3093\u3060\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B3\u30C8\u30AE \u30DE\u30CA"
    paragraphs: "\u8131\u51FA\u306A\u3093\u3066\u305D\u3093\u306A\u3053\u3068\u672C\u5F53\u306B\u3067\u304D\u308B\u306E\u2026\uFF1F"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u3067\u304D\u308B\u304B\u306F\u308F\u304B\u3089\u306A\u3044\u3002\u3060\u3051\u3069\u63A2\u3055\u306A\u304D\u3083\u898B\u3064\u304B\u308B\u3053\u3068\u306F\u7121\u3044\u3093\u3060\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u3072\u3068\u307E\u305A\u76EE\u7684\u306F\u7384\u95A2\u30DB\u30FC\u30EB\u306E\u6249\u3092\u958B\u304F\u65B9\u6CD5\u3092\u63A2\u3059\u3053\u3068\u304B\u306A\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B3\u30C8\u30AE \u30DE\u30CA"
    paragraphs: "\u9014\u4E2D\u306B2\u968E\u3078\u306E\u968E\u6BB5\u304C\u3042\u3063\u305F\u3093\u3060\u3051\u3069\u3002\u9244\u683C\u5B50\u304C\u3042\u3063\u3066\u901A\u308C\u306A\u304B\u3063\u305F\u3088\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u306A\u30892\u968E\u3078\u884C\u304F\u65B9\u6CD5\u3082\u898B\u3064\u3051\u306A\u3044\u3068\u3044\u3051\u306A\u3044\u306A\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u307F\u3093\u306A\u3067\u624B\u5206\u3051\u3057\u3066\u3001\u3053\u306E\u5B66\u5712\u3092\u8ABF\u3079\u308B\u3093\u3060\u2026\uFF01"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u671F\u9650\u306F2\u65E5\u5F8C\u306E\u591C\u304B\u2026\u8AB0\u3082\u6B7B\u306A\u305B\u306F\u3057\u306A\u3044\uFF01"
    colorType: 0
    windowType: 0
