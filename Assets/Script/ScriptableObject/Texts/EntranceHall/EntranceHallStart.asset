    colorType: 0
    windowType: 0
  - speakerName: "\u30CB\u30AB\u30A4\u30C9\u30A6 \u30CF\u30EB\u30CE"
    paragraphs: "\u672C\u5F53\u306F\u306D\u2026\u8D85\u9AD8\u6821\u7D1A\u306E\u2026\u624B\u54C1\u5E2B\u306A\u3093\u3060\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u624B\u54C1\u5E2B\uFF1F\u30DE\u30B8\u30C3\u30AF\u304C\u3067\u304D\u308B\u3063\u3066\u3053\u3068\u304B\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30CB\u30AB\u30A4\u30C9\u30A6 \u30CF\u30EB\u30CE"
    paragraphs: "\u305D\u3046\u3060\u3088\u3001\u4EBA\u4F53\u5207\u65AD\u304B\u3089\u6C34\u4E2D\u8131\u51FA\u307E\u3067\u306A\u3093\u3067\u3082\u3054\u3056\u308C\u3055\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B3\u30C8\u30AE \u30DE\u30CA"
    paragraphs: "\u3042\u3063\uFF01\u3058\u3083\u3042\u3055\u3001\u4ECA\u306A\u306B\u304B\u62AB\u9732\u3067\u304D\u308B\u3082\u306E\u3063\u3066\u3042\u308B\u304B\u306A\u3002\n\u4E8C\u968E\u5802\u3055\u3093\u306E\u624B\u54C1\u898B\u3066\u307F\u305F\u3044\u3067\u3059\uFF01"
    colorType: 0
    windowType: 0
  - speakerName: "\u30CB\u30AB\u30A4\u30C9\u30A6 \u30CF\u30EB\u30CE"
    paragraphs: "\u624B\u54C1\u306B\u306F\u7A2E\u3068\u4ED5\u639B\u3051\u304C\u5927\u4E8B\u3060\u304B\u3089\u306D\u2026\u30C8\u30E9\u30F3\u30D7\u304C\u4E00\u679A\u3082\u306A\u3044\u4ECA\u306F\u7121\u7406\u304B\u306A\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30B7\u30CE\u30CE\u30E1 \u30C8\u30AA\u30EB"
    paragraphs: "\u305D\u3046\u304B\u2026\u305D\u308C\u306F\u6B8B\u5FF5\u3060\u306A\u3002"
    colorType: 0
    windowType: 0
  - speakerName: "\u30CB\u30AB\u30A4\u30C9\u30A6 \u30CF\u30EB\u30CE"
    paragraphs: "\u3042\u3042\u3067\u3082\u3001\u3061\u3087\u3063\u3068\u3057\u305F\u96A0\u3057\u82B8\u306A\u3089\u3067\u304D\u308B\u3088\u3002"
