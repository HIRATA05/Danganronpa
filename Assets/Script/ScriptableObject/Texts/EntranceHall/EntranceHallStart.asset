  - speakerName: "\u30CB\u30AB\u30A4\u30C9\u30A6 \u30CF\u30EB\u30CE"
    paragraphs: "\u3075\u3075\u3001\u5197\u8AC7\u3060\u3088\u3002\n\u5C11\u3057\u304B\u3089\u304B\u3063\u3066\u307F\u305F\u3060\u3051\u3060\u3088\u2026"
    colorType: 0
